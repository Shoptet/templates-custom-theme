{
  "name": "shoptet-template",
  "version": "0.1.11",
  "author": "Václav Vracovský, https://www.shoptet.cz/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Shoptet/templates-custom-theme.git"
  },
  "bugs": {
    "url": "https://github.com/Shoptet/templates-custom-theme/issues"
  },
  "templates": {
    "10": {
      "name": "Waltz",
      "url": "https://waltz.shoptet.cz/"
    },
    "11": {
      "name": "Classic",
      "url": "https://classic.shoptet.cz/"
    },
    "12": {
      "name": "Step",
      "url": "https://step.shoptet.cz/"
    },
    "13": {
      "name": "Disco",
      "url": "https://disco.shoptet.cz/"
    },
    "07": {
      "name": "Techno",
      "url": "https://techno.shoptet.cz/"
    },
    "09": {
      "name": "Tango",
      "url": "https://tango.shoptet.cz/"
    }
  },
  "mainCss": {
    "options": {
      "javascriptEnabled": true,
      "compress": true
    },
    "files": {
      "dist/main.css": "../assets/11/css/main.less"
    }
  },
  "projectCss": {
    "options": {
      "compress": true
    },
    "files": {
      "dist/project.css": "../assets/11/css/project.less"
    }
  },
  "fontCss": {
    "options": {
      "compress": true,
      "modifyVars": {
        "fontPath": "./"
      }
    },
    "files": {
      "dist/font-shoptet.css": "../assets/11/css/font-shoptet.less"
    }
  },
  "printCss": {
    "options": {
      "compress": true
    },
    "files": {
      "dist/print.css": "../assets/11/css/print.less"
    }
  },
  "concatJS": {
    "options": {
      "separator": ";"
    },
    "src": [
      "../assets/00/js/modules/jquery.unveil.js",
      "../assets/00/js/bootstrap/transition.js",
      "../assets/00/js/bootstrap/carousel.js",
      "../assets/00/js/bootstrap/dropdown.js",
      "../assets/00/js/bootstrap/tab.js",
      "../assets/00/js/bootstrap/tooltip.js",
      "../assets/00/js/jqueryui/core.js",
      "../assets/00/js/jqueryui/widget.js",
      "../assets/00/js/jqueryui/mouse.js",
      "../assets/00/js/jqueryui/position.js",
      "../assets/00/js/jqueryui/autocomplete.js",
      "../assets/00/js/jqueryui/button.js",
      "../assets/00/js/jqueryui/datepicker.js",
      "../assets/00/js/jqueryui/i18n/datepicker-cs.js",
      "../assets/00/js/jqueryui/menu.js",
      "../assets/00/js/jqueryui/selectmenu.js",
      "../assets/00/js/jqueryui/slider.js",
      "../assets/00/js/modules/jquery.ui.touch-punch.js",
      "../assets/00/js/modules/jquery.colorbox.js",
      "../assets/libs/jquery-ajax-response/jquery.ajax.response.js",
      "../assets/libs/polyfills.js",
      "../assets/libs/scripts.js",
      "../assets/libs/dev.js",
      "../assets/libs/cookie.js",
      "../assets/libs/helpers.js",
      "../assets/libs/tracking.js",
      "../assets/libs/image360.js",
      "../assets/libs/stockAvailabilities.js",
      "../assets/libs/variants/common.js",
      "../assets/libs/variants/simple.js",
      "../assets/libs/variants/split.js",
      "../assets/libs/ajax.js",
      "../assets/libs/cart.js",
      "../assets/libs/phoneInput.js",
      "../assets/libs/validator/validator.js",
      "../assets/libs/validator/phone.js",
      "../assets/libs/validator/required.js",
      "../assets/00/js/modules/validator.js",
      "../assets/00/js/modules/jquery.cloud-zoom.1.0.2.js",
      "../assets/00/js/modules/cloudZoomInit.js",
      "../assets/00/js/modules/tabsResponsive.js",
      "../assets/00/js/modules/topNavigationMenu.js",
      "../assets/00/js/modules/globalFunctions.js",
      "../assets/00/js/modules/menu.js",
      "../assets/00/js/modules/products.js",
      "../assets/00/js/modules/search.js",
      "../assets/00/js/modules/filters.js",
      "../assets/00/js/modules/productInnerSecondImage.js",
      "../assets/libs/checkout.js",
      "../assets/00/js/modules/twisto.js"
    ],
    "dest": "dist/build.js"
  },
  "uglifyJS": {
    "options": {
      "mangle": false
    },
    "files": {
      "dist/build.min.js": ["dist/build.js"]
    }
  },
  "devDependencies": {
    "grunt": "^1.0.4",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-less": "^2.0.0",
    "grunt-contrib-uglify": "^4.0.1"
  }
}
